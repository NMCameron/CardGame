import Troops.Archer;
public class Main {

	public static void main(String[] args) {

		Archer arch = new Archer(5);
		
		System.out.println("Name: " + arch.getTroopName());
		System.out.println("Level: " + arch.getLevel());
	    System.out.println("Hit Points: " + arch.getHitPoints());
	    System.out.println("Attack Damage: " + arch.getAttackDamage());
	    System.out.println("Movement Speed: " + arch.getMovementSpeed());
	    System.out.println("Attack Range: " + arch.getAttackRange());
	    System.out.println("Can Attack Troops: " + arch.checkCanAttackTroops());
	}

}
