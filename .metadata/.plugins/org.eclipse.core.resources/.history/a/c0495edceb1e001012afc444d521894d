import Troops.Archer;
public class Main {

	public static void main(String[] args) {

		Archer arch = new Archer(5);
		
		System.out.println("Name: " + arch.getTroopName() + "Level: " + arch.getLevel() + "Hit Points: " + arch.getHitPoints() 
		+ "Attack Damage: " + arch.getAttackDamage() + "Movement Speed: " + arch.getMovementSpeed() + "Attack Range: " 
		+ arch.getAttackRange() + "Can Attack Troops: " + arch.checkCanAttackTroops());
	}

}
